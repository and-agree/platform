{
    "name": "functions",
    "version": "0.0.1",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run lint && tsc && copyfiles -u 1 src/**/*.html lib",
        "build:watch": "tsc --watch",
        "lint": "eslint --ext .js,.ts .",
        "test": "npm run build && npm run test:ci",
        "test:run": "cucumber-js --require ./test ./test/features --exit",
        "test:ci": "firebase emulators:exec --project=andagree-testing \"npm run test:run\""
    },
    "private": true,
    "main": "lib/index.js",
    "engines": {
        "node": "14"
    },
    "dependencies": {
        "@sendgrid/mail": "7.4.7",
        "busboy": "0.3.1",
        "ejs": "3.1.6",
        "firebase-admin": "9.11.1",
        "firebase-functions": "3.15.7"
    },
    "devDependencies": {
        "@cucumber/cucumber": "7.3.1",
        "@firebase/testing": "0.20.11",
        "@types/busboy": "0.2.4",
        "@types/ejs": "3.1.0",
        "@typescript-eslint/eslint-plugin": "4.31.2",
        "@typescript-eslint/parser": "4.31.2",
        "chai": "4.3.4",
        "copyfiles": "2.4.1",
        "eslint": "7.32.0",
        "eslint-config-google": "0.14.0",
        "eslint-plugin-import": "2.24.2",
        "faker": "5.5.3",
        "firebase-functions-test": "0.3.2",
        "nock": "13.1.3",
        "node-fetch": "2.6.3",
        "typescript": "4.3.5"
    }
}
